<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/tornado/cronjob.xsd">
	<import>
		<cronjob name="dev.dmedia.AutomaticGroupRemoval.userGroupRemoval">
			<classname>wcf\system\cronjob\UserGroupRemovalCronjob</classname>
			<description>Removes users automatically from user groups</description>
			<description language="de">Entfernt Benutzer automatisch aus Benutzergruppen</description>
			<startminute>*/10</startminute>
			<starthour>*</starthour>
			<startdom>*</startdom>
			<startmonth>*</startmonth>
			<startdow>*</startdow>
			<canbeedited>1</canbeedited>
			<canbedisabled>1</canbedisabled>
		</cronjob>
	</import>
